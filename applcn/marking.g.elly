p:pxnorv  # set typd to indicate nclu or vclu
_
# trace
# view 32
  if typd=n
    append <nclu>
  else
    append <vclu>
    end
  split  
  left
  back
# view 24
  if typd=n
    find <nclu> <
  else
    find <vclu> <
    end
  (rcur)  # strip <nclu>...</nclu> pairs in split buffer
  merge
  linefeed
  append <punc>
  right
  append </punc>
  if typd=n
    append </nclu>
  else
    append </vclu>
    end
__
p:rcur   # recursive deletion subprocedure
_
# view 16
  delete 6 >
  if typd=n
    find </nclu> <
  else
    find </vclu> <
    end
  delete 7 >
  peek x <
  if x LF
    shift 2 <   # take accompanying space also
    end
  extract nxt < 6
  insert < nxt
# show nxt
  if nxt=<nclu>, <vclu>
    (rcur)
    end
__

